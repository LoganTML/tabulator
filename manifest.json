{
    "manifest_version": 3,
    "name": "Tabulator",
    "version": "1.0",
    "description": "A tab cleanup extension",
    "permissions": ["tabs", "storage", "activeTab", "declarativeContent", "scripting", "contextMenus"],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "scripts/background.js"
    }
}
  